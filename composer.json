{
  "name": "nominatepro/app",
  "type": "project",
  "require": {
    "php": ">=8.2",
    "ext-pdo": "*",
    "bcosca/fatfree": "^3.8",
    "bcosca/fatfree-core": "^3.8",
    "level-2/dice": "^4.0",
    "mailgun/mailgun-php": "^4.3",
    "nyholm/psr7": "^1.8",
    "php-di/php-di": "^6.3",
    "symfony/http-client": "^5.4",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Support/renderHtml.php",
      "app/Support/Flash.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
